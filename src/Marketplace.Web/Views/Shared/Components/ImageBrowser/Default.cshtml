@model string[]

<input type="file" name="images" id="images" multiple />

@if (Model is not null && Model.Any())
{
    <div class="image-browser">
        @for (int i = 0; i < Model.Length; i++)
        {
            <img src="@Model[i]" id="img_@i" />
        }
    </div>
}
else
{
    <div class="image-browser"></div>
}